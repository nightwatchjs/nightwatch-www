<script>(function (p, h) {
    p = p.replace(/\/api\/.+\.html$/, '/api/$method');
    p = p.replace(/\/blog\/.+\.html$/, '/blog/article');
    var q = p.split('/');
    q.shift();
    h.setAttribute('data-uri', (p !== '/' ? '/' + q.join('/') : p));
  })(location.pathname, document.documentElement);
</script>

